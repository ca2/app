<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
<Project>
		<Option title="aurapresshook" />
		<Option pch_mode="2" />
  		<Option compiler="gcc" />
		<Build>
			<Target title="basis">
				<Option output="$(WORKSPACEDIR)/../../time-$(#platform_modifier)/x64/$(TARGET_NAME)/aurapresshook" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(WORKSPACEDIR)/../../time-$(#platform_modifier)/x64/$(TARGET_NAME)/" />
				<Option object_output="$(WORKSPACEDIR)/../../time-$(#platform_modifier)/intermediate/$(TARGET_NAME)/$(PROJECT_NAME)" />
				<Option type="1" />
		 		<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_DEBUG" />
				</Compiler>
			</Target>
			<Target title="memdleak">
				<Option output="$(WORKSPACEDIR)/../../time-$(#platform_modifier)/x64/$(TARGET_NAME)/lib$(PROJECT_NAME).so" prefix_auto="0" extension_auto="0" />
				<Option working_dir="$(WORKSPACEDIR)/../../time-$(#platform_modifier)/x64/$(TARGET_NAME)/" />
				<Option object_output="$(WORKSPACEDIR)/../../time-$(#platform_modifier)/intermediate/$(TARGET_NAME)/$(PROJECT_NAME)" />
				<Option type="3" />
		 		<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-g" />
					<Add option="-D_DEBUG" />
					<Add option="-DMEMDLEAK" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++0x" />
			<Add option="-fexceptions" />
			<Add option="-fnon-call-exceptions" />
			<Add option="-fno-pie" />
			<Add option="-fno-stack-protector" />
			<Add option="-Wno-pointer-arith" />
			<Add option="-Wno-attributes" />
			<Add option="-Winvalid-pch" />
			<Add option="-Wno-conversion-null" />
			<Add option="-fPIC" />
			<Add option="`pkg-config --cflags gtk+-2.0`" />
			<Add directory="." />
			<Add directory="$(PROJECT_DIR)" />
			<Add directory="$(#archive)" />
			<Add directory="$(#config)" />
			<Add directory="$(WORKSPACE_DIR)/../.." />
			<Add directory="$(WORKSPACE_DIR)/../../inc" />
			<Add directory="$(WORKSPACE_DIR)/../../include" />
			<Add directory="$(WORKSPACE_DIR)/../../platform-linux" />
			<Add directory="$(WORKSPACE_DIR)/../../platform-linux/_include" />
			<Add directory="$(WORKSPACE_DIR)/../../app/_include" />
			

			<Add directory="../../include" />

			
			
			<Add directory="$(WORKSPACE_DIR)/../../app" />
			<Add directory="../..core" />

			<Add directory="../../thirdparty" />
			<Add directory="../../thirdparty" />


		</Compiler>
		<Linker>
			<Add option="-Wl,-rpath=\\$$$ORIGIN" />
			
			<Add option="-lappindicator" />
			<Add option="`pkg-config --cflags --libs gtk+-2.0`" />
			<Add option="-Wl,-z,defs" />
			<Add directory="$(WORKSPACEDIR)/../../time-$(#platform_modifier)/x64/$(TARGET_NAME)" />
			<Add directory="/usr/lib64/mysql/" />
		</Linker>
		<Unit filename="aurapresshook.cpp" />
		<Unit filename="aurapresshook.h" />
		<Unit filename="basecore.cpp" />
		<Unit filename="basecore.h" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
